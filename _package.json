{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "Awesome site created with choo-genesis",
  "main": "src/app.js",
  "scripts": {
    "copy": "mkdir -p dist/assets/style && cp index.html dist/ && cp src/assets/style/main.css dist/assets/style",
    "dev:less": "lessc src/assets/style/main.less src/assets/style/main.css",
    "dev:js": "budo src/app.js --open --live --pushstate --host 0.0.0.0 --port 8080 --",
    "dev:sw": "sw-precache --static-file-globs='dist/**/*.*'",
    "dev": "npm run dev:sw && gazer -p 'src/assets/style/**/*.less' npm run dev:less & npm run dev:js",
    "test": "standard --verbose | snazzy && NODE_ENV=test mochify",
    "build:js": "NODE_ENV=production browserify -e src/app.js -o dist/app.js -t envify -t sheetify/transform -g uglifyify | uglifyjs",
    "build:sw": "sw-precache --root=dist --static-file-globs='dist/**/*.*'",
    "build": "npm run copy && npm run build:js && npm run build:sw"
  },
  "keywords": [
    "choo",
    "generator",
    "scaffold"
  ],
  "browserify": {
    "transform": [
      "sheetify/transform",
      "babelify"
    ]
  },
  "author": "<%= auhtor %>",
  "license": "MIT",
  "dependencies": {
    "choo": "^3.2.2",
    "localforage": "^1.4.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^9.0.0",
    "choo-log": "^1.4.0",
    "choo-test": "0.0.5",
    "component-emitter": "^1.2.1",
    "envify": "^3.4.1",
    "gazer": "0.0.3",
    "insert-css": "^1.0.0",
    "less": "^2.7.1",
    "mochify": "^2.18.1",
    "sheetify": "^5.1.0",
    "snazzy": "^4.0.1",
    "standard": "^8.0.0",
    "sw-precache": "^4.0.0",
    "uglifyify": "^3.0.2",
    "uglifyjs": "^2.4.10"
  }
}
